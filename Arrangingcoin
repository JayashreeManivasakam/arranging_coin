import java.util.Scanner; 

class HelloWorld {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n  =sc.nextInt();
    int count=0;
    int i=0;
    if(n>0){
    for( i=1;i<=n;i++){
        if(n>=i){
            count++;
        }
        else
        System.out.print(i-1);
        n=n-i;
    }
    System.out.print(count);
    
           }
    else
    System.out.print("Enter a valid number");
    }
}
